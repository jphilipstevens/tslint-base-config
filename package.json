{
  "name": "@jphilipstevens/tslint-base-config",
  "version": "2.0.0",
  "main": "tslint.json",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jphilipstevens/tslint-base-config.git"
  },
  "engines": {
    "node": ">8",
    "npm": ">=5.2"
  },
  "files": [
    "tslint.json"
  ],
  "scripts": {
    "lint": "npx jsonlint tslint.json -q",
    "commit": "npx git-cz",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^6.0.1",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.0.6",
    "@semantic-release/npm": "^5.1.10",
    "@semantic-release/release-notes-generator": "^7.0.2",
    "condition-circle": "^2.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "semantic-release": "^17.2.3"
  },
  "peerDependencies": {
    "tslint": "^5.5.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^2.2.0 || ^3.0.0"
  }
}
